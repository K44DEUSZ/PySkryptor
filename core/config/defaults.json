{
  "paths": {
    "resources_dir": "resources",
    "ffmpeg_subdir": "ffmpeg",
    "models_subdir": "models",
    "ai_engine_subdir": "whisper-turbo",
    "data_dir": "data",
    "downloads_subdir": "downloads",
    "input_tmp_subdir": ".input_tmp",
    "transcriptions_subdir": "transcriptions"
  },
  "media": {
    "audio_ext": [".wav", ".mp3", ".flac", ".ogg", ".m4a", ".aac", ".wma", ".alac", ".aiff", ".opus", ".amr", ".mp2"],
    "video_ext": [".mp4", ".mkv", ".webm", ".mov", ".avi", ".flv", ".wmv", ".mpeg", ".mpg", ".m4v", ".3gp"]
  },
  "model": {
    "ai_engine_name": "whisper-turbo",
    "local_models_only": true,
    "chunk_length_s": 60,
    "stride_length_s": 5,
    "pipeline_task": "transcribe",
    "ignore_warning": true,
    "default_language": null,
    "return_timestamps": true,
    "use_safetensors": true,
    "low_cpu_mem_usage": true
  },
  "user": {
    "language": "pl",
    "preferred_device": "auto",
    "precision": "auto",
    "allow_tf32": true,
    "timestamps_output": true,
    "keep_downloaded_files": true,
    "keep_wav_temp": false
  }
}
